{
    "name": "decodelabs/fabric",
    "description": "Lightweight PHP framework implementation using DecodeLabs ecosystem",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "repositories": {
    },
    "require": {
        "php": "^8.4",

        "decodelabs/archetype": "^0.3.10",
        "decodelabs/clip": "^0.5.5",
        "decodelabs/coercion": "^0.3.3",
        "decodelabs/commandment": "^0.2.2",
        "decodelabs/dovetail": "^0.2.14",
        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/genesis": "^0.12.4",
        "decodelabs/glitch": "^0.20",
        "decodelabs/greenleaf": "^0.8.4",
        "decodelabs/harvest": "^0.5.4",
        "decodelabs/lucid": "^0.5.2",
        "decodelabs/monarch": "^0.1.4",
        "decodelabs/pandora": "^0.3.3",
        "decodelabs/systemic": "^0.11.18",
        "decodelabs/veneer": "^0.12.8"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7",
        "decodelabs/stash": "^0.6.5"
    },
    "conflict": {
        "decodelabs/stash": "<0.6"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Fabric\\": "src/"
        },
        "files": [
            "src/Context.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.8.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
